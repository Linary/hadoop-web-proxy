<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="configuration.xsl"?>
<configuration>
	<property>
		 <name>hadoop.tmp.dir</name>
		 <value>file:/opt/hadoop-2.6.4/tmp</value>
		 <description>Abase for other temporary directories.</description>
	</property>
	<property>
		<name>fs.defaultFS</name>
		<value>hdfs://master:8020</value>
	</property>
	<property>
		<name>io.file.buffer.size</name>
		<value>131072</value>
	</property>
	<property>
		<name>hadoop.proxyuser.linary.hosts</name>
		<value>*</value>
	</property>
	<property>
		<name>hadoop.proxyuser.linary.groups</name>
		<value>*</value>
	</property>
</configuration>
